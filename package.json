{
    "name": "@dhconnelly/parents",
    "version": "1.2.0",
    "description": "a small interpreter for a lisp-like language",
    "scripts": {
        "test": "mocha -r ts-node/register test/test_*.ts",
        "dev": "node --loader ts-node/esm ./src/bin/cli.ts",
        "build": "rm -rf dist && npx tsc && chmod +x dist/src/bin/cli.js",
        "clean": "rm -rf dist"
    },
    "bin": {
        "parents": "./dist/src/bin/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dhconnelly/parents.git"
    },
    "keywords": [
        "lisp",
        "typescript",
        "interpreter"
    ],
    "author": "Daniel Connelly <dhconnelly@gmail.com> (https://dhconnelly.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dhconnelly/parents/issues"
    },
    "homepage": "https://github.com/dhconnelly/parents#readme",
    "devDependencies": {
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.41",
        "mocha": "^8.4.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    }
}
